<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.junaid.transpire">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_fit"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_fit"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".splash">
            android:theme="@style/Theme.AnApp.Logo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".loginActivity"
            android:label="@string/app_name" />
        <activity android:name=".signup" />
        <activity android:name=".activity_register" />
        <activity
            android:name=".home_activity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".profile" />
        <activity android:name=".nav_header" />
        <activity android:name=".ListExercise" />
        <activity android:name=".ViewExercise" />
        <activity android:name=".SettingsPage" />
        <receiver android:name=".AlarmNotificationReceiver" />

        <activity android:name=".list_days" />
        <activity android:name=".Daily_Training"></activity>
    </application>

</manifest>